<%= form_for :user, url: '/users' do |f| %>
  <%= f.label :user_name %>
  <%= f.text_field :name %>

  <%= f.label :user_email %>
  <%= f.text_field :email %>

  <%= f.label :user_password %>
  <%= f.password_field :password %>

  <%= f.label :user_password_confirmation %>
  <%= f.password_field :password_confirmation %>

  <%= f.submit "Create My Account" %>

<% end %>
